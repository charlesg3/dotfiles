{
    "document": {
        "block_prefix": "\n",
        "block_suffix": "\n",
        "margin": 2
    },
    "block_quote": {
        "indent": 1,
        "indent_token": "│ ",
        "color": "${PANDA_COMMENT}"
    },
    "paragraph": {},
    "list": {
        "level_indent": 2
    },
    "heading": {
        "block_suffix": "\n",
        "bold": true,
        "underline": true,
        "color": "${PANDA_LIME}"
    },
    "h1": {
        "block_suffix": "\n",
        "bold": true,
        "underline": true,
        "color": "${PANDA_LIME}"
    },
    "h2": {
        "block_suffix": "\n",
        "bold": true,
        "underline": true,
        "color": "${PANDA_LIME}"
    },
    "h3": {
        "bold": true,
        "underline": false,
        "color": "${PANDA_GREEN}"
    },
    "h4": {
        "bold": true,
        "underline": true,
        "color": "${PANDA_LIME}"
    },
    "h5": {
        "bold": true,
        "underline": true,
        "color": "${PANDA_LIME}"
    },
    "h6": {
        "bold": true,
        "underline": true,
        "color": "${PANDA_LIME}"
    },
    "strong": {
        "bold": true,
        "color": "${PANDA_LIGHT_PINK}"
    },
    "emph": {
        "italic": true,
        "color": "${PANDA_LIGHT_PINK}"
    },
    "hr": {
        "color": "${PANDA_COMMENT}"
    },
    "item": {
        "block_prefix": "• "
    },
    "enumeration": {
        "block_prefix": ". "
    },
    "task": {
        "ticked": "✓ ",
        "unticked": "○ "
    },
    "code": {
        "color": "${PANDA_LAVENDER}"
    },
    "code_block": {
        "color": "${PANDA_FG}",
        "background_color": "${PANDA_SELECTION}",
        "indent": 1,
        "margin": 2
    },
    "table": {
        "center_separator": "┼",
        "column_separator": "│",
        "row_separator": "─"
    },
    "table_row": {},
    "table_cell": {},
    "link": {
        "color": "${PANDA_BLUE}",
        "underline": true
    },
    "link_text": {
        "color": "${PANDA_BLUE}",
        "bold": true
    },
    "image": {
        "color": "${PANDA_BLUE}",
        "underline": true
    },
    "html_block": {},
    "html_span": {}
}
